<td><%= model.type_i18n %></td>
<td>
  <p><%= model.working_directory %></p>
  <p class="text-danger"><%= model.base_name %></p>
</td>
<td><%= model.organ&.name %></td>
<td>
<% if model.github_user %>
  <p><%= model.github_user.name %></p>
<% end %>
</td>
<td>
  <p><%= model.last_commit_message %></p>
  <p class="text-grey"><time data-controller="time" data-format="human"><%= model.last_commit_at&.to_fs(:iso8601) %></time></p>
</td>
